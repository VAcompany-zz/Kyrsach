<Application x:Class="Kyrsach.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Kyrsach"
             xmlns:ser="clr-namespace:Kyrsach.Services"
             StartupUri="View\Windows\HomeWindow.xaml" Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resurser/Themes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <ser:VMLocator x:Key="vmlocator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>